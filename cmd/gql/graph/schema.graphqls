# This could be generified somehow
type Depiction {
  res: String
}

type User {
  UID: String!
  username: String!
  Password: String!
  Token: [String]
}

input Registration {
  username: String!
  password: String!
}

input Login {
  username: String!
  password: String!
}

type Query {
  ping: String!
  authorisedPing: String!
}

type Mutation {
  register(input: Registration!): Depiction!
  login(input: Login!): Depiction!
}